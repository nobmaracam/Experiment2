//ITEM NUMBER 6
#include <iostream>
#include <conio.h>
using namespace std;
int main(){
	
	int num1 = 0;
	int num2 = 1;
	int temp;
	
	cout<<num1<<","<<num2<<",";
	for(int i = 3; i <= 22; i++){
		temp = num1 + num2;
		num1 = num2;
		num2 = temp;
		cout<<temp;
		
		if(i < 22){
			cout<<",";
		}
	}
	return 0,
	_getch();
}
